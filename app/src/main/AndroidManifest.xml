<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andreas.goserve">

    <application
        android:allowBackup="true"
        android:icon="@drawable/menu_branch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Dashboard" />
        <activity android:name=".MejaPelanggan"
            android:parentActivityName=".Dashboard"/>
        <activity android:name=".ReportMenu"
            android:parentActivityName=".Dashboard"/>
        <activity
            android:name=".StockMenu"
            android:label="@string/title_activity_stock_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".BranchMenu"></activity>
    </application>

</manifest>